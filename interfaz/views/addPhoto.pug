extends layout

block content
    .w3-card-4
        header.w3-container.w3-black
            h2 Add Photo
        
        form.w3-container(method="POST" enctype="multipart/form-data")
            fieldset
                label Image ofthe street
                input.w3-input.w3-round(type="file" name="myFile")
                label(style="margin-left:50px") Description
                input.w3-input.w3-round(type="text" style="margin-left:50px" name="description")
            button.w3-btn.w3-black.w3-mb-2(type="submit") Add
        footer.w3-container.w3-black
            h5 Generated by Mercurio Verde Hopson